namespace utterances

import com.amazon.alexa.ask.conversations.utterances
import types.SpecifyFavoriteColorRequestType

RecordFavoriteColor = utterances(
  samples = [
    "store my favorite color",
    "save my favorite color"
  ]
)

GetFavoriteColor = utterances(
  samples = [
    "get my favorite color",
    "what is my favorite color"
  ]
)

